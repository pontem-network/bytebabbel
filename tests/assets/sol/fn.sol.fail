pragma solidity ^0.8.0;

library Fn {
    // # fn_const()
    function fn_const() public pure{ }

    function test_1(uint a, uint b, uint c) public pure returns (uint){
        if (a == 0) {
            return b;
        }else{
            return c;
        }
    }

    // # test_3 (0, 1, 2) 1
    function test_3(uint a, uint b, uint c) public pure returns (uint){
        bool stop = a < 1;
        if (stop) {
            return b;
        }

        return b + c;
    }
}
